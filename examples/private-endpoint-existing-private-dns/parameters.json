{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "privateEndpointName": {
            "value": "<PRIVATE_ENDPOINT_NAME>"
        },
        "privateEndpointResourceId": {
            "value": "/subscriptions/<SUBSCRIPTION_ID>/resourceGroups/<RESOURCE_GROUP_NAME>/providers/<RESOURCE_TYPE>/<RESOURCE>/<RESOURCE_NAME>"
        },
        /* Sub resource such as "blob" or "queue" */
        "privateEndpointSubResource": {
            "value": [
                "<SUB_RESOURCE>"
            ]
        },
        "virtualNetworkName": {
            "value": "<VNET_NAME>"
        },
        "subnetName": {
            "value": "<SUBNET_NAME>"
        },
        "privateDnsZoneName": {
            "value": "<PRIVATE_DNS_ZONE_NAME>"
        },
        "tags": {
            "value": {
                "TAG1": "VALUE",
                "TAG2": "VALUE"
            }
        }
    }
}